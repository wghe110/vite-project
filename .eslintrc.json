{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "html"
  ],
  "rules": {
    "vue/multi-word-component-names": 1,
    "no-unused-vars": 1,
    "no-debugger": 1,
    "vue/comment-directive": 0
  }
}