{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "html"
  ],
  "rules": {
    "vue/multi-word-component-names": 0,
    "no-unused-vars": 1,
    "no-debugger": 1,
    "vue/comment-directive": 0,
    "react/react-in-jsx-scope": 0
  }
}