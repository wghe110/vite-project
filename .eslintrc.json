{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [],
  "rules": {
    "vue/multi-word-component-names": 0,
    "no-unused-vars": 1,
    "no-debugger": 1,
    "vue/comment-directive": 0,
    "react/react-in-jsx-scope": 0
  }
}